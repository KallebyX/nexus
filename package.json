{
  "name": "@oryum/nexus",
  "version": "1.0.0",
  "description": "Framework modular da Oryum Tech para desenvolvimento acelerado",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "create": "node cli/create-project.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd templates/frontend && npm run dev",
    "dev:backend": "cd templates/backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd templates/frontend && npm run build",
    "build:backend": "cd templates/backend && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "docs": "node scripts/generate-docs.js",
    "deploy": "node scripts/deploy.js",
    "setup": "node scripts/setup.js",
    "ai:docs": "node scripts/ai-docs-generator.js",
    "ai:tests": "node scripts/ai-test-generator.js",
    "security:check": "node scripts/security-checker.js",
    "backup": "node scripts/backup.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset-db.js",
    "health:check": "node scripts/health-check.js"
  },
  "keywords": [
    "framework",
    "modular",
    "oryum",
    "react",
    "nextjs",
    "nodejs",
    "automation",
    "ai-powered"
  ],
  "author": "Oryum Tech",
  "license": "MIT",
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@slack/web-api": "^6.9.0",
    "@supabase/supabase-js": "^2.39.0",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autocannon": "^7.12.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "inquirer": "^9.2.11",
    "jest": "^29.7.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.0.0",
    "mongodb": "^6.2.0",
    "next": "^14.0.4",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.8",
    "selenium-webdriver": "^4.15.0",
    "semver": "^7.5.0",
    "sequelize": "^6.35.0",
    "stripe": "^14.5.0",
    "supertest": "^6.3.3",
    "tar": "^6.2.0",
    "twilio": "^4.19.0",
    "web-push": "^3.6.0",
    "winston": "^3.11.0",
    "zaproxy": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.36.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OryumTech/nexus"
  },
  "bin": {
    "nexus": "./cli/nexus.js"
  }
}
